package variable.practice;

public class CarDriver {
	public static void main(String[] args) {
		Car c1 = new Car();
		c1.color = "red";
		c1.brand = "A";
		c1.price = 12000.00;
		c1.mileage=41.5;
		System.out.println(c1.color);
		System.out.println(c1.brand);
		System.out.println(c1.price);
		System.out.println(c1.mileage);
		Car c2 = new Car();
		c1.color = "yellow";
		c1.brand = "B";
		c1.price = 14000.00;
		c1.mileage=43.5;
		System.out.println(c2.color);
		System.out.println(c2.brand);
		System.out.println(c2.price);
		System.out.println(c2.mileage);
		Car c3 = new Car();
		c1.color = "black";
		c1.brand = "C";
		c1.price = 16000.00;
		c1.mileage=48.5;
		System.out.println(c3.color);
		System.out.println(c3.brand);
		System.out.println(c3.price);
		System.out.println(c3.mileage);
	}
}
